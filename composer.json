{
  "name": "alexdesignworks/drupal_helpers",
  "description": "Helper classes for Drupal",
  "keywords": [
    "drupal",
    "helper",
    "utility"
  ],
  "homepage": "https://github.com/alexdesignworks/drupal_helpers",
  "license": "GPL-2.0+",
  "authors": [
    {
      "name": "Alex Skrypnyk",
      "email": "alex.designworks@gmail.com"
    },
    {
      "name": "Nick Santamaria",
      "email": "nick@nicksantamaria.net"
    }
  ],
  "support": {
    "issues": "https://github.com/alexdesignworks/drupal_helpers/issues",
    "source": "https://github.com/alexdesignworks/drupal_helpers"
  },
  "require-dev": {
    "phpunit/phpunit": "4.8.*",
    "drupal/coder": "*",
    "beet/box": "^0.7.2"
  },
  "autoload": {
    "psr-0": {
      "Drupal\\drupal_helpers\\": "lib/"
    }
  },
  "scripts": {
    "cs": "vendor/bin/phpcs --runtime-set installed_paths $(pwd)/vendor/drupal/coder/coder_sniffer --colors -s -p",
    "cbf": "vendor/bin/phpcbf --runtime-set installed_paths $(pwd)/vendor/drupal/coder/coder_sniffer --colors",
    "test": "vendor/bin/phpunit"
  }
}
